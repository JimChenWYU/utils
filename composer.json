{
    "name": "jimchen/utils",
    "description": "常用工具集",
    "type": "library",
    "license": "MIT",
    "suggest": {
        "ext-mbstring": "*",
        "ext-ctype": "*",
        "ext-json": "*",
        "ext-simplexml": "*"
    },
    "require": {
        "guzzlehttp/guzzle": "^6.3",
        "danielstjules/stringy": "^3.1"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.8",
        "mockery/mockery": "0.9.x-dev"
    },
    "autoload": {
        "psr-4": {
            "JimChen\\Utils\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "JimChen\\Utils\\Tests\\": "tests"
        }
    }
}
